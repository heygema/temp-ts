{
  "extends": "tsconfig-kodefox",
  "compilerOptions": {
    "module": "commonjs",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": "./build",
    "typeRoots": [
      "node_modules/@types",
      "custom_types"
    ] /* List of folders to include type definitions from. */
  }
}
